<%= form_for([@tictactoe, @player]) do |f| %>        
        
  <%= f.label :name %>  
  <%= f.text_field :name %>

  <%= f.label :mark %>  
  <%= f.text_field :mark %>

  <%= f.label :first_to_act %>  
  <%= f.text_field :first_to_act %>  

  <%= f.submit "Create Player", class: "btn btn-large btn-primary" %>
<% end %>



